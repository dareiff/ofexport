{
    "indent": 0,
    "dateFormat": "%Y-%m-%d-%a",
    "depth": 0, 
    "Nodes": {
        "ProjectStart": "$indent$name:", 
        "FolderStart": "$indent$name:", 
        "ContextStart": "$indent$name:",
        "TaskStart": "$indent- $name$date_completed", 
        "TaskGroupStart": "$indent$name:$date_completed"
    }, 
    "NodeAttributeDefaults": {
        "date_to_start": "", 
        "date_due": "", 
        "date_completed": "", 
        "link" : "",
        "name": "", 
        "project": "", 
        "context": "", 
        "flagged": "",
        "status": "", 
        "note": ""
    }, 
    "indentString": "\t", 
    "NodeAttributes": {
        "date_to_start": " @start($value)", 
        "date_due": " @due($value)", 
        "date_completed": " @$value", 
        "link": "$value", 
        "name": "$value", 
        "status": " @status($value)", 
        "project": " @project($value)", 
        "context": " @context($value)", 
        "flagged": " @flagged",
        "note": "note"
    }
}
